<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <dllmap dll="SDL3" os="windows" target="SDL3.dll"/>
    <dllmap dll="SDL3" os="osx" target="libSDL3.0.dylib"/>
    <dllmap dll="SDL3" os="linux,freebsd,netbsd" target="libSDL3.so.0"/>

    <dllmap dll="FAudio" os="windows" target="FAudio.dll"/>
    <dllmap dll="FAudio" os="osx" target="libFAudio.0.dylib"/>
    <dllmap dll="FAudio" os="linux,freebsd,netbsd" target="libFAudio.so.0"/>
    
    <dllmap dll="Wellspring" os="windows" target="Wellspring.dll"/>
    <dllmap dll="Wellspring" os="osx" target="libWellspring.1.dylib"/>
    <dllmap dll="Wellspring" os="linux,freebsd,netbsd,openbsd" target="libWellspring.so.1"/>

    <dllmap dll="IRO" os="windows" target="IRO.dll"/>
    <dllmap dll="IRO" os="osx" target="libIRO.1.dylib"/>
    <dllmap dll="IRO" os="linux,freebsd,netbsd,openbsd" target="libIRO.so.1"/>

    <dllmap dll="SDL3_shadercross" os="windows" target="SDL3_shadercross.dll" dependsOn="spirv-cross,dxcompiler"/>
    <dllmap dll="SDL3_shadercross" os="osx" target="libSDL3_shadercross.0.dylib" dependsOn="spirv-cross,dxcompiler"/>
    <dllmap dll="SDL3_shadercross" os="linux,freebsd,netbsd,openbsd" target="libSDL3_shadercross.so.0"
            dependsOn="spirv-cross,dxcompiler"/>

    <dllmap dll="spirv-cross" os="windows" target="spirv-cross-c-shared.dll"/>
    <dllmap dll="spirv-cross" os="osx" target="libspirv-cross-c-shared.0.dylib"/>
    <dllmap dll="spirv-cross" os="linux,freebsd,netbsd,openbsd" target="libspirv-cross-c-shared.so.0"/>

    <dllmap dll="dxcompiler" os="windows" target="dxcompiler.dll"/>
    <dllmap dll="dxcompiler" os="osx" target="libdxcompiler.dylib"/>
    <dllmap dll="dxcompiler" os="linux,freebsd,netbsd,openbsd" target="libdxcompiler.so.0"/>
</configuration>